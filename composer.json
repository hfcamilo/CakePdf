{
	"name": "hfcamilo/cake-pdf",
	"type": "cakephp-plugin",
        "description": "CakePHP plugin for creating and/or rendering Pdf, several Pdf engines supported. Forked to get the dependecies with composer",
	"homepage": "http://github.com/ceeram/CakePdf",
	"license": "MIT",
	"require": {
		"php": ">=5.3.0",
        "dompdf": "dev-master"
	},
    "repositories": {
        "dompdf": {
            "type": "package",
            "package": {
                "name": "dompdf",
                "version": "dev-master",
                "dist": {
                    "url": "https://codeload.github.com/dompdf/dompdf/zip/master",
                    "type": "zip"
                }
            }
        }
    },
	"extra": {
		"installer-name": "CakePdf"
	},
    "config": {
        "vendor-dir": "Vendor"
    }
}